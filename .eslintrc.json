{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint-config-airbnb-base/legacy",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "never"],
    "linebreak-style": ["error", "windows"],
    "comma-dangle": ["error", "only-multiline"],
    "no-param-reassign": ["off"],
    "no-plusplus": "off",
    "object-curly-newline": ["off"]
  }
}
